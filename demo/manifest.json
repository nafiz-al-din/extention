{
    "manifest_version": 2,
    "name": "hello world",
    "version": "0.0.1",
    "description": "tis is the demo version of the file",
    "icons": {
        "16":"images/16x.png",
        "32":"images/32x.png",
        "48":"images/48x.png",
        "128":"images/128x.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "bookmarks",
        "storage"
    ],
    "browser_action": {
        "default_icon": {
            "16":"images/16x.png",
            "32":"images/32x.png"
        },
        "default_title": "My hello world extention"
    }
}